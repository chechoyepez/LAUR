    SUBROUTINE TSR197983.AIA.COR
**===================================================================================================================================
* Program to update incorrect period from AA.INTEREST.ACCRUALS
*
**===================================================================================================================================
    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_F.AA.INTEREST.ACCRUALS
**===================================================================================================================================
    GOSUB INIT
    GOSUB PROCESS	

    RETURN
**===================================================================================================================================
INIT:
	
    FN.AIA = "F.AA.INTEREST.ACCRUALS"
    F.AIA = ""
    CALL OPF(FN.AIA, F.AIA)

    FN.SL = "&SAVEDLISTS&"
    F.SL = ""
    CALL OPF(FN.SL,F.SL)

    	

    RETURN
**===================================================================================================================================
PROCESS:

    AA.ID1 = 'AA19019YM8YQ'
    AIA.ID = AA.ID1:"-PRINCIPALINT"
    PRINT "Processing of AA19019YM8YQ"
    R.AIA = ""
    AIA.ERR = ""

    CALL F.READ(FN.AIA, AIA.ID, R.AIA, F.AIA, AIA.ERR)
    
    IF NOT(AIA.ERR) AND R.AIA THEN                  
	R.AIA<AA.INT.ACC.LINKED.RATE.CHANGE> = ''
        WRITE R.AIA TO F.AIA,AIA.ID 
    END  

    AA.ID1 = 'AA19019YM8YQ'
    AIA.ID = AA.ID1:"-ESCROWCRINT"

    R.AIA = ""
    AIA.ERR = ""

    CALL F.READ(FN.AIA, AIA.ID, R.AIA, F.AIA, AIA.ERR)
    
    IF NOT(AIA.ERR) AND R.AIA THEN                  
	R.AIA<AA.INT.ACC.LINKED.RATE.CHANGE> = ''
        WRITE R.AIA TO F.AIA,AIA.ID 
    END 

    PRINT "Process completed for AA19019YM8YQ"

    RETURN
**===================================================================================================================================
*
**===================================================================================================================================

